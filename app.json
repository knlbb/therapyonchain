{
  "expo": {
    "name": "my-app",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.my-app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-health"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.fdylan.myapp"
    },
    "extra": {
      "eas": {
        "projectId": "968c2b89-73f2-4b9f-b7b2-182547bab925"
      }
    }
  }
}
